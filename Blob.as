package {	import flash.display.MovieClip;	import flash.geom.*		public class Blob extends MovieClip {				public function Blob(centre:Point) {			this.centre = centre;		}				public function get centre():Point{			return new Point(this.x, this.y);		}				public function set centre(centre:Point):void {			this.x = centre.x;			this.y = centre.y;		}				public function get radius():Number{			return this.width / 2;		}	}}